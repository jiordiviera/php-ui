{
    "name": "jiordiviera/php-ui",
    "description": "A shadcn-like CLI for Laravel Livewire components.",
    "license": "MIT",
    "keywords": ["laravel", "livewire", "ui", "components", "scaffold", "cli", "shadcn", "tallstack"],
    "homepage": "https://github.com/jiordiviera/php-ui",
    "support": {
        "issues": "https://github.com/jiordiviera/php-ui/issues",
        "source": "https://github.com/jiordiviera/php-ui"
    },
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/prompts": "^0.1",
        "illuminate/filesystem": "^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Jiordiviera\\PhpUi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jiordi Viera (jiordiviera)",
            "email": "jiordikengne@gmail.com"
        }
    ],
    "bin": [
        "bin/php-ui"
    ]
}