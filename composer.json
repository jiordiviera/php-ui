{
    "name": "jiordiviera/php-ui",
    "description": "A shadcn-like CLI for Laravel Livewire components.",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire",
        "ui",
        "components",
        "scaffold",
        "cli",
        "shadcn",
        "tallstack"
    ],
    "homepage": "https://github.com/jiordiviera/php-ui",
    "support": {
        "issues": "https://github.com/jiordiviera/php-ui/issues",
        "source": "https://github.com/jiordiviera/php-ui"
    },
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/prompts": "^0.1|^0.2|^0.3",
        "illuminate/filesystem": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "laravel/pint": "^1.13"
    },
    "scripts": {
        "test": "pest",
        "lint": "pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Jiordiviera\\PhpUi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jiordi Viera (jiordiviera)",
            "email": "jiordikengne@gmail.com"
        }
    ],
    "bin": [
        "bin/php-ui"
    ]
}